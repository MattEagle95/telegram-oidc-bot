generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Chat {
  id               String    @id @default(uuid())
  oidcCodeVerifier String?
  oidcAccessToken  String?
  oidcExpiresAt    DateTime?
  updatedAt        DateTime  @updatedAt
  createdAt        DateTime  @default(now())
}
